<!--Jest to widok tymczasowy, zalezy od frontu jak chca dostac te dane-->

@using ProjektIO.Models
@model ViewModels
@{
    ViewBag.Title = "JoinGroupError";
}


<div class="header">
    <div class="row align-items-center">
        <div class="col col-12 col-sm-9">
            <h1 class="text-center">@Model.Group.Nazwa</h1>
        </div>
        <img src="~/Content/Images/kolo.png" class="img-thumbnail rounded header-logo m-auto" style="position: relative; left: -25px; bottom: -10px;">
    </div>
    <div style="padding: 10px;"></div>
    <div class="content">
        @RenderPage("~/Views/Group/GroupNav.cshtml")
    </div>
</div>

<div style="margin-top: 1%;" class="alert alert-danger">
    Już należysz do tego koła, albo prośba została wysłana i oczekuje na akceptację przez administratora!
</div>