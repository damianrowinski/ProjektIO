@using ProjektIO.Models
@model ViewModels

@{
    ViewBag.Title = "ShowPortfolio";
}

<div class="header" id="image" style="background-image: url('@Model.Group.SciezkaDoObrazu');background-size: 100% 100%">
    <div class="row align-items-center">
        <div class="col">
            <h1 class="text-center">@Model.Group.Nazwa</h1>
        </div>
    </div>
    <div style="padding: 100px;"></div>
</div>
<div class="content" style="position: relative; padding-bottom: 0px;">

    @RenderPage("~/Views/Group/GroupNav.cshtml")
</div>

<h1 style="padding-top: 1%; padding-bottom: 1%;">Porfolio koła</h1>

<div class="card" style="color: white;">
    <div class="card-body bg-dark">

        @Model.Portfolio.Zawartosc
    </div>
    <div class="card-footer">
        @Model.Portfolio.DataUtworzenia
    </div>
</div>


