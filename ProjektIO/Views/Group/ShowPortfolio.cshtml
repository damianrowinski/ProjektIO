@using ProjektIO.Models
@model ViewModels

@{
    ViewBag.Title = "ShowPortfolio";
}

<div class="header">
    <div class="row align-items-center">
        <div class="col col-12 col-sm-9">
            <h1 class="text-center">@Model.Portfolio.KoloNaukowe.Nazwa</h1>
        </div>
        <img src="~/Content/Images/kolo.png" class="img-thumbnail rounded header-logo m-auto" style="position: relative; left: -25px; bottom: -10px;">
    </div>
    <div style="padding: 10px;"></div>
    <div class="content">
        @RenderPage("~/Views/Group/GroupNav.cshtml",Model)
    </div>
</div>

<h1 style="padding-top: 1%; padding-bottom: 1%;">Porfolio koła</h1>

<div class="card" style="color: white;">
    <div class="card-body bg-dark">
        
        @Model.Portfolio.Zawartosc  
    </div>
    <div class="card-footer">
        @Model.Portfolio.DataUtworzenia
    </div>
</div>


